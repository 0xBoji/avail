  "dependencies": {
    "@polkadot/api": "^7.5.1"
  },
  "devDependencies": {
    "@polkadot/typegen": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
